#linear search in c

#include<stdio.h> 
int main() 
{ 
    int n,a[100],i,ele; 
    scanf("%d",&n); 
    for(i=0;i<n;i++) 
    { 
        scanf("%d",&a[i]); 
    } 
    scanf("%d",&ele); 
    for(i=0;i<n;i++) 
    { 
        if(a[i]==ele) 
        { 
            printf("%d is found at %d index...",ele,i); 
            return 0; 
        } 
    } 
    printf("%d is not found ...",ele); 
    return 0; 
}
