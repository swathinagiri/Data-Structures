# bubble sort algorithm



#include<stdio.h> 
int main() 
{ 
    int i,n,a[100],j; 
    scanf("%d",&n); 
    for(i=0;i<n;i++) 
    { 
         scanf("%d",&a[i]); 
    } 
    for(i=0;i<n;i++) 
    { 
        for(j=0;j<n-i-1;j++) 
        { 
            if(a[j]>a[j+1]) 
            { 
                  a[j]=a[j+1]+a[j]; 
                 a[j+1]=a[j]-a[j+1]; 
                 a[j]=a[j]-a[j+1]; 
            } 
        } 
    } 
    printf("After Bubble sort:"); 
    for(i=0;i<n;i++) 
    { 
        printf("%d ",a[i]); 
    } 
}
